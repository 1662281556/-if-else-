#include <stdio.h>
int main()
{
    int score = 50000;
    //完善一下代码
    if (score>=10000)
    {
        printf("钻石玩家");
    }
    else if (score>=5000&&score<10000)
    {
        printf("白金玩家");
    }
    else if (score>=1000&&score<5000)
    {
        printf("青铜玩家");
    }
    else//最后一个else后面不写东西，第一个if中间全都是else if最后一个else。
    {
        printf("普通玩家");
    }
    return 0;
}
